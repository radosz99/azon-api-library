@misc{48241,
  authors =	"Jadwiga Brzuchowska, Elżbieta Litwińska, Tomasz Ossowicz, Justyna Maśko-Osiadacz",
  title =	"Możliwości powiązań wybranych technik wspomagania planowania przestrzennego bazą danych typu GIS. Etap II",
  type =	"inny dokument",
  note =	"Niniejszy raport opracowano w ramach serii badań realizowanych w Katedrze Planowania Przestrzennego dotyczących rozwoju metod komputerowego wspomagania gospodarki przestrzennej. Praca miała na celu przygotowanie teoretyczne i praktyczne testowanie budowy złożonego systemu informacji przestrzennej obejmującego oprócz zbiorów danych (zdefiniowanych w oparciu o silny program typu GIS) powiązanych z metodami ich wprowadzania, aktualizacji, agregacji i zestawiania, także narzędzia przetwarzania tych danych oparte na modelach komunikacyjnych i alokacyjnych.
Wykorzystano dane dostępne dzięki współpracy Wydziału Architektury Politechniki Wrocławskiej i Urzędu Miejskiego Wrocławia. Część z nich stanowiły materiały "Studium uwarunkowań i kierunków zagospodarowania gminy Wrocław", opracowanego w latach 1996-1997 pod kierownictwem prof. Tadeusza Zipsera przez zespół pracowników Katedry Planowania Przestrzennego we współpracy z zespołem Biura Rozwoju Wrocławia."
}

@misc{48245,
  authors =	"Jadwiga Brzuchowska, Elżbieta Litwińska, Tomasz Ossowicz",
  title =	"Możliwości powiązań wybranych technik wspomagania planowania przestrzennego bazą danych typu GIS. Etap I",
  type =	"inny dokument",
  note =	"Raport z serii kompleksowych prac mających na celu m.in. doskonalenie metod wspomagających planowanie przestrzenne i podejmowanie decyzji w gospodarce przestrzennej. W szczególności przewidziano opracowanie złożonego systemu informacji przestrzennej obejmującego oprócz zbiorów danych (zdefiniowanych w oparciu o silny program typu GIS) powiązanych z metodami ich wprowadzania, aktualizacji, agregacji i zestawiania, także narzędzia przetwarzania tych danych oparte na modelach komunikacyjnych i alokacyjnych. Przewidziano równoległe (poza przedstawianym zleceniem) prace nad doskonaleniem i rozwojem pakietu modeli komunikacyjnych oraz nad rozszerzeniem zastosowań baz typu GIS w gospodarce przestrzennej. W ramach tego zlecenia zaplanowano zaprojektowanie i przetestowania ww. systemu na możliwie szerokim i realnym przykładzie (a w miarę możliwości - na kilku przykładach). Założono, że prace będą prowadzone w kilku iteracjach: doskonalenia systemu i testowania go. W pierwszym etapie przewidziano prace dotyczące zarówno zdefiniowania i przetestowania pierwszych, najprostszych wersji systemu, jak i rozpoczęcia przygotowania wersji zaawansowanych."
}

@misc{48267,
  authors =	"Ryszard Żabiński, Tadeusz Zipser, Jerzy Chmiel, Tomasz Ossowicz, Janusz Bułat, Elżbieta Litwińska, Regina Maga-Jagielnicka, Mirosław Markiewicz, Michał Mońka, Andrzej Nędzi, Tadeusz Sawa-Borysławski, Anna Zalewska",
  title =	"Kierunki rozwoju aglomeracji poznańskiej: studium optymalizacji",
  type =	"inny dokument",
  note =	"Niniejsze opracowanie zawiera kompleksowy opis metod i danych, które posłużyły do stworzenia kierunków rozwoju aglomeracji poznańskiej na zlecenie Biura Planowania Przestrzennego w Poznaniu w roku 1980. Skorzystano tu z paradygmatu decyzji przestrzennych, tworząc swoisty model symulujący decyzje planistyczne i na jego podstawie zmierzono się z zagadnieniami odpowiedniego planowania aktywności, kontaktów oraz konfliktów."
}

